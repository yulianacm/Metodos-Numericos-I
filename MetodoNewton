      "source": [
        "# Método de Newton\n",
        "def newton_method(p0, tol, max_i):\n",
        "    p = p0\n",
        "    errores=[]  #Lista vacía para guardar errores\n",
        "    print(\"Iteración\\tp_n-1\\tf(p_n-1)\\tf'(p_n-1)\\tp_n\\t\\tError\")     #Imprimir parte de la tabla\n",
        "\n",
        "    for i in range(max_i):\n",
        "        f_p = f(p)\n",
        "        df_p = df(p)\n",
        "\n",
        "        if abs(df_p) < 1e-10:\n",
        "            print(\"La derivada es demasiado pequeña.\")\n",
        "            return None\n",
        "\n",
        "        p_next= p - f_p/df_p\n",
        "        error= abs(p_next-p)  #ERROR\n",
        "        errores.append(error)\n",
        "        print(\"{}\\t\\t{:.3f}\\t{:.3f}\\t\\t{:.3f}\\t\\t{:.3f}\\t\\t{:.3f}\".format(\n",
        "            i+1, p, f_p, df_p, p_next, error))\n",
        "\n",
        "        if error<tol:\n",
        "            print(\"Solución encontrada: {} en {} iteraciones. \".format(p_next, i+1))\n",
        "            graficar_errores(errores)\n",
        "            return p_next\n",
        "        p= p_next\n",
        "\n",
        "    print(\"El método no convergió en el número máximo de iteraciones.\")\n",
        "    graficar_errores(errores)\n",
        "    return None\n",
        "\n",
        "# Llamamos al método\n",
        "solucion = newton_method(p0, tol, max_i)\n",
        "print(\"Solución aproximada: {}\".format(solucion))"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "def graficar_errores(errores):\n",
        "    plt.plot(range(1, len(errores) + 1), errores)\n",
        "    plt.xlabel('Iteración')\n",
        "    plt.ylabel('Error')\n",
        "    plt.title('Error en cada iteración')\n",
        "    plt.grid(True)\n",
        "    plt.show()"
      ],
