      ],
      "source": [
        "# Definir función para evaluar un polinomio usando el método de Horner\n",
        "def horner(coef, x):\n",
        "    result = coef[0]\n",
        "    for i in range(1, len(coef)):\n",
        "        result = result * x + coef[i]\n",
        "    return result\n",
        "\n",
        "# Definir función para la derivada usando Horner\n",
        "def hornerderi(coef, x):\n",
        "    n = len(coef) - 1  # Grado del polinomio\n",
        "    derivcoef = [coef[i] * (n - i) for i in range(n)]  # Coeficientes de la derivada\n",
        "    return horner(derivcoef, x)\n",
        "\n",
        "# Polinomio P(x) = 2x^4 - 3x^2 + 3x - 4\n",
        "coefpolinomio = [2, 0, -3, 3, -4]  # Coeficientes del polinomio de mayor a menor grado\n",
        "\n",
        "# Método de Newton con Horner\n",
        "def nh(p0, tol, max_i, coef):\n",
        "    p = p0\n",
        "    errores = []  # Lista para almacenar los errores\n",
        "    print(\"Iteración\\tp_n-1\\tP(p_n-1)\\tP'(p_n-1)\\tp_n\\t\\tError\")  # Imprimir la tabla\n",
        "\n",
        "    for i in range(max_i):\n",
        "        f_p = horner(coef, p)  # Evaluar el polinomio en p usando Horner\n",
        "        df_p = hornerderi(coef, p)  # Evaluar la derivada en p usando Horner\n",
        "\n",
        "        if abs(df_p) < 1e-10:  # Verificar si la derivada es muy pequeña\n",
        "            print(\"La derivada es demasiado pequeña.\")\n",
        "            return None\n",
        "\n",
        "        p_next = p - f_p / df_p\n",
        "        error = abs(p_next - p)  # Calcular el error\n",
        "        errores.append(error)\n",
        "\n",
        "        print(f\"{i+1}\\t\\t{p:.5f}\\t{f_p:.5f}\\t{df_p:.5f}\\t{p_next:.5f}\\t{error:.5f}\")\n",
        "\n",
        "        if error < tol:\n",
        "            print(f\"Solución encontrada: {p_next} en {i+1} iteraciones.\")\n",
        "            graficar_errores(errores)\n",
        "            return p_next\n",
        "\n",
        "        p = p_next\n",
        "\n",
        "    print(\"El método no convergió en el número máximo de iteraciones.\")\n",
        "    graficar_errores(errores)\n",
        "    return None\n",
        "# Función para graficar los errores\n",
        "def graficar_errores(errores):\n",
        "    plt.plot(errores, marker='o')\n",
        "    plt.title(\"Convergencia del método de Newton\")\n",
        "    plt.xlabel(\"Iteración\")\n",
        "    plt.ylabel(\"Error\")\n",
        "    plt.yscale(\"log\")\n",
        "    plt.grid(True)\n",
        "    plt.show()\n",
        "# Llamar al método de Newton usando Horner\n",
        "solucion = nh(p0, tol, max_i, coefpolinomio)\n",
        "print(f\"Solución aproximada: {solucion}\")\n"
      ]
    }
  ]
